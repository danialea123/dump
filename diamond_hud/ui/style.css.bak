@keyframes animate-circle {
    0% {
        opacity: 0.10;
        stroke-dashoffset: 800;
    }

    30% {
        opacity: 0.30;
        stroke-dashoffset: 200;
    }

    80% {
        opacity: 0.80;
        stroke-dashoffset: 100;
    }

    100% {
        opacity: 10;
    }
}

