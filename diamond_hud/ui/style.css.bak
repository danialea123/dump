    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: -5px;

    overflow: hidden;
}

#fuelIcon {
    position: absolute;
    bottom: 285px;
    right: 170px;
}

#toolsIcon {
    position: absolute;
    bottom: 240px;
    right: 230px;
}

.engineLevel {
    box-sizing: border-box;
    position: absolute;
    width: 220px;
    height: 220px;
    border-radius: 50%;
    top: 40px;
    left: 83px;

    border-top: 8px solid rgba(0, 0, 0, 0);
    border-left: 8px solid rgba(0, 0, 0, 0);
    border-right: 8px solid rgba(0, 0, 0, 0);
    border-bottom: 8px solid rgba(0, 0, 0, 0);

    transform: rotate(-45deg);
}

.slicerFuel {
    height: 200px;
    width: 200px;
    position: absolute;

    top: -75px;
    right: -35px;
    transform: rotate(-5deg);
    overflow: hidden;
}

.fuelLevelMarker {
    box-sizing: border-box;
    position: absolute;
    width: 286px;
    height: 286px;
    border-radius: 50%;
    top: 70px;
    left: -180px;

    border-right: 6px solid #2a2a2a;
    border-bottom: 6px solid #ffa02f;
    border-top: 6px solid rgba(0, 0, 0, 0);
    border-left: 6px solid rgba(0, 0, 0, 0);
    transition: all .5s linear;
    /*  transform: rotate(-120deg); */

    animation: fuel-up 8s ease;
}

/* 
@keyframes fuel-up {
    from {
        transform: rotate(-50deg)
    }
    to {
        transform: rotate(-120deg)
    }  
}
*/
.slicerEngine {
    height: 90px;
    width: 90px;
    position: absolute;

    top: 54px;
    left: 54px;
    transform: rotate(-16deg);
    overflow: hidden;
}

.engineLevelMarker {
    box-sizing: border-box;
    position: absolute;
    width: 220px;
    height: 220px;
    border-radius: 50%;
    top: 10px;
    left: 10px;

    border-top: 6px solid #2a2a2a;
    border-left: 6px solid #2d4dbc;
    border-right: 6px solid rgba(0, 0, 0, 0);
    border-bottom: 6px solid rgba(0, 0, 0, 0);



    transition: all .5s linear;
    /*  transform: rotate(30deg); */
    /*  animation: engine-reduce 4s ease; */
}

@keyframes engine-reduce {
    0% {
        transform: rotate(-30deg)
    }

    100% {
        transform: rotate(30deg)
    }
}

#main {
    width: 270px;
    height: 270px;
    /* background-color: rgba(0, 0, 0, 0.185); */
}

* {
    margin: 0;
    padding: 0;
}

p {
    white-space: nowrap;
}

.velocityCircle {
    fill: none;
    stroke-width: 14;
    stroke: #2A2A2A;
    stroke-opacity: 1;
    transform: translate(10px, 10px);

    stroke-dasharray: 800;
    stroke-dashoffset: 800;
}

circle:nth-child(1) {
    stroke-dashoffset: 0;

}

circle:nth-child(2) {
    /*   stroke-dashoffset: calc(800 - (800 * 100) / 100); */
    stroke: #01FF66;
    animation: animate-circle 4s cubic-bezier(0, 0.23, 1, .1);
}

#healthProgress,
#armourProgress,
#oxigenProgress,
#thirstProgress,
#hungerProgress,
#energyProgress {
    float: right;
    /*  border-top-right-radius: 17px; */
    /*  border-top-left-radius: 17px; */
    /*  border-bottom-right-radius: 17px; */
    /*  border-bottom-left-radius: 17px; */
    border-radius: 17px;
    overflow: hidden;
    height: 100%;
    transition: all .5s linear;
}

#healthProgress {
    /*   width: 70%; */
    background: linear-gradient(90deg, #01FF66 -193.75%, #2EA05B 100%);
}

#armourProgress {
    /*  width: 70%; */
    background: linear-gradient(90deg, #0194FF -27.07%, #020E26 192.32%);
}

#thirstProgress {
    /*  width: 70%; */
    background: linear-gradient(90deg, #635DE2 0%, #4C46D2 100%);
}

#hungerProgress {
    /*  width: 70%; */
    background: linear-gradient(90deg, #C8955B -5.17%, #8C663C 100%);
}

#energyProgress {
    /*  width: 70%; */
    background: linear-gradient(90deg, #2AC793 0%, #37AD86 100%);
}

#oxigenProgress {
    /*  width: 70%; */
    background: linear-gradient(90deg, #0194FF 0%, #035B9B 100%);

}

