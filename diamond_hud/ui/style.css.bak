.new-hud {
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
    direction: rtl;
    margin-right: 1vw;
}

.new-hud h3 {
    color: #1558eb;
    font-size: 1.5vw;
    font-weight: bold;
    font-style: italic;
    margin-bottom: 1.5vw;
    animation: rgb 10s infinite alternate ;

}

.new-hud ul {
    list-style: none;
}

.new-hud ul li {
    font-style: italic;
    font-size: 1vw;
    color: #fff;
    font-weight: bold;
}


.new-hud ul li:nth-child(1) i {
    color: #0060fc;
}

.new-hud ul li:nth-child(2) i {
    color: #fd0d29;
}

.new-hud ul li:nth-child(3) i {
    color: #fd6300;
}

.new-hud ul li:nth-child(4) i {
    color: #00fd7f;
}

.new-hud ul li:nth-child(5) i {
    color: #a9fd7f;
}

.new-hud ul li:nth-child(6) i {
    color: #faee0a;
}

.new-hud ul li:nth-child(5) span i{
    color: #fff !important;
}

@keyframes rgb {
    0% {
        color: #0060fc;
    }
    20% {
        color: #fd0d29;
    }
    40% {
        color: #fd6300;
    }
    60% {
        color: #00fd7f;
    }
    80% {
        color: #a9fd7f;
    }
    100% {
        color: #faee0a
    }
}